{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://gh-proxy.com/https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "direct",
      "default_mode": "rule"
    },
  "cache_file": {
      "enabled": true,
      "store_fakeip": false
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "proxyDns",
        "address": "tls://8.8.8.8",
        "detour": "Proxy"
      },
      {
        "tag": "localDns",
        "address": "https://223.5.5.5/dns-query",
        "detour": "direct"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "localDns"
      },
      {
        "rule_set": "geosite-cn",
        "server": "localDns"
      },
      {
        "clash_mode": "direct",
        "server": "localDns"
      },
      {
        "clash_mode": "global",
        "server": "proxyDns"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "proxyDns"
      }
    ],
    "final": "localDns",
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": [
        "172.19.0.0/30"
      ],
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "stack": "system",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      }
    },
    {
      "tag": "mixed-in",
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 2080
    }
  ],
  "outbounds": [
    {
      "tag": "Proxy",
      "type": "selector",
      "outbounds": [
        "🎈 自动选择",
        "🇸🇬 新加坡自动",
        "🇭🇰 香港自动",
        "🇺🇸 美国自动",
        "🇯🇵 日本自动",
        "🇹🇼 台湾自动",
        "{all}"
      ],
      "default": "🇸🇬 新加坡自动"
    },
    {
      "tag": "🎈 自动选择",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "300s",
      "tolerance": 5
    },
    {
      "tag": "Youtube",
      "type": "selector",
      "outbounds": [
        "🇸🇬 新加坡自动",
        "🇺🇸 美国自动",
        "🇭🇰 香港自动",
        "Others",
        "Proxy",
        "{all}"
      ],
      "default": "🇸🇬 新加坡自动"
    },
    {
      "tag": "OpenAI",
      "type": "selector",
      "outbounds": [
        "🇹🇼 台湾自动",
        "🇸🇬 新加坡自动",
        "🇯🇵 日本自动",
        "🇺🇸 美国自动",
        "Proxy",
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "🇭🇰|HK|hk|香港|港|HongKong"
          ]
        }
      ]
    },
    {
      "tag": "Spotify",
      "type": "selector",
      "outbounds": [
        "🇺🇸 美国自动",
        "🇯🇵 日本自动",
        "Proxy",
        "{all}"
      ],
      "default": "🇺🇸 美国自动"
    },
    {
      "tag": "TikTok",
      "type": "selector",
      "outbounds": [
        "🇸🇬 新加坡自动",
        "🇭🇰 香港自动",
        "🇺🇸 美国自动",
        "🇯🇵 日本自动",
        "Proxy",
        "{all}"
      ],
      "default": "🇺🇸 美国自动"
    },
    {
      "tag": "Meta / Ig",
      "type": "selector",
      "outbounds": [
        "🇸🇬 新加坡自动",
        "🇭🇰 香港自动",
        "🇺🇸 美国自动",
        "🇯🇵 日本自动",
        "Proxy",
        "{all}"
      ],
      "default": "🇺🇸 美国自动"
    },
    {
      "tag": "Global",
      "type": "selector",
      "outbounds": [
        "Proxy",
        "🇸🇬 新加坡自动",
        "🇭🇰 香港自动",
        "🇺🇸 美国自动",
        "🇯🇵 日本自动",
        "🇹🇼 台湾自动",
        "direct",
        "{all}"
      ],
      "default": "Proxy"
    },
    {
      "tag": "🇭🇰 香港自动",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇭🇰|HK|hk|香港|港|HongKong"
          ]
        }
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "300s",
      "tolerance": 5
    },
    {
      "tag": "🇹🇼 台湾自动",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇹🇼|TW|tw|台湾|臺灣|台|Taiwan"
          ]
        }
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "300s",
      "tolerance": 5
    },
    {
      "tag": "🇸🇬 新加坡自动",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇸🇬|SG|sg|新加坡|狮|Singapore"
          ]
        }
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "300s",
      "tolerance": 5
    },
    {
      "tag": "🇯🇵 日本自动",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇯🇵|JP|jp|日本|日|Japan"
          ]
        }
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "300s",
      "tolerance": 5
    },
    {
      "tag": "🇺🇸 美国自动",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇺🇸|US|us|美国|美|United States"
          ]
        }
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "300s",
      "tolerance": 5
    },
    {
      "tag": "Others",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "🇭🇰|HK|hk|香港|香|🇹🇼|TW|tw|台湾|台|🇸🇬|SG|sg|新加坡|狮|🇯🇵|JP|jp|日本|日|🇺🇸|US|us|美国|美"
          ]
        }
      ]
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "Proxy",
    "rules": [
      {
        "inbound": ["tun-in", "mixed-in"],
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules":[
          {
            "port":53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "clash_mode": "rule",
        "action": "reject"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "clash_mode": "global",
        "outbound": "Proxy"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "global",
        "outbound": "Proxy"
      },
      {
        "domain": [
          "clash.razord.top",
          "yacd.metacubex.one",
          "yacd.haishan.me",
          "d.metacubex.one"
        ],
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-youtube",
        "outbound": "Youtube"
      },
      {
        "rule_set": "geosite-openai",
        "outbound": "OpenAI"
      },
      {
        "rule_set": "geosite-spotify",
        "outbound": "Spotify"
      },
      {
        "rule_set": "geosite-tiktok",
        "outbound": "TikTok"
      },
      {
        "rule_set": "geosite-meta",
        "outbound": "Meta / Ig"
      },
      {
        "rule_set": "geosite-instagram",
        "outbound": "Meta / Ig"
      },
      {
        "rule_set": "geoip-facebook",
        "outbound": "Meta / Ig"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "outbound": "Global"
      }
    ],
    "rule_set": [
      {
        "tag": "geosite-category-ads-all",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-ads-all.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-openai",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/Toperlock/sing-box-geosite@main/rule/OpenAI.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-youtube",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/youtube.srs",
        "download_detour": "direct"
      },
      {
